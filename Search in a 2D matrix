class Solution {
    public boolean searchMatrix(int[][] mat, int k) {
        int n=mat.length;
        int m=mat[0].length;
        if(n==0 || m==0){
            return false;
        }
        for(int i=0;i<n;i++){
            for(int j=0;j<m;j++){
                if(mat[i][j]==k)return true;
            
            }
        }
        return false;
    }
}
